.oe24app{background-color:#ffffff}
.socialSharing {
    width: 100%;
    display: table;
    table-layout: fixed;
    border-spacing: 2px;
}
.socialSharing .entry {
    display: table-cell;
    max-width: 32%;
    text-align: center;
}
.socialSharing .entry img {
    max-width: 100%;
}
.storyFbIframe {
    width:95%;
    margin: 0 auto;
}

iframe {
    border: none;
}

.articleBox .storyBottom {
  margin: 0 auto;
}

