    max-width: 100%;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.contentBox.console .storyTitle {
    font-family: "Open Sans Condensed";
    font-size: 1.8125rem;
    font-weight: 700;
    letter-spacing: -1pt;
    line-height: 1;
    display: block;
}
.contentBox.console .storyLeadText {
    /*font-family: "Open Sans";*/
