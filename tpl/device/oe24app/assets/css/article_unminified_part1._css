}
.contentBox.console .story {
    padding: 0;
    margin: 0;
    position: absolute;
    left: 0;
    top: 0;
    z-index: -1;
}
.contentBox.console .story:first-child {
    position: static;
}


.contentBox.console .story .imageContainer {
    /* aspect ratio 2:1 */
    width: 100%;
    padding-bottom: 50%;
    /*padding-bottom: 62.58%;*/
    position: relative;
}
.contentBox.console .story .imageContainer img {
    display: block;
    background-color: #d5d5d5;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    /*height: 100%;*/
    /*height: auto;*/
