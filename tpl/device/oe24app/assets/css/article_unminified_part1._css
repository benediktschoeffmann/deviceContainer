.mobileTeaserOe24Tv.collapsed .teaserText {
    width: 92%;
}
.mobileTeaserOe24Tv.collapsed .teaserFigure {
    display: none;
}
.mobileTeaserOe24Tv.collapsed .teaserPreTitle {
    margin: 0 0 4px;
}
.mobileTeaserOe24Tv.collapsed .teaserIcon {
    top: 4px;

    /* border */
    border: none;
    /* border end */

    max-height: 100%;

    position: absolute;
    right: 0;
    top: 50%;
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    transform: translateY(-50%);
}
.mobileTeaserOe24Tv.collapsed .teaserIconCross {
    display: none;
}
.mobileTeaserOe24Tv.collapsed .teaserIconPlus {
    display: block;
}


