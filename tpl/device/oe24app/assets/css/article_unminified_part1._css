/* _base.css */
html {
    box-sizing: border-box;
}
*,
*:before,
*:after {
    box-sizing: inherit;
}

/* Reset padding and margin for all elements */
* {
    padding: 0;
    margin: 0;
}

html,
body {
    /*width: 100%;*/
    /*min-height: 100%;*/
}

body {
    font-family: 'Open Sans', sans-serif;
    font-size: 16px;
    font-style: normal;
    font-weight: 400;
    line-height: 1.4;
    color: #000;
    background-color: #f1f1f1;
    /* http://icomoon.io/#docs */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.wrapper {
    width: 100%;
}

ul,
li {
    list-style-type: none;
}

h1,
h2,
h3,
h4,
h5,
h6 {
    font-weight: 700;
    line-height: 1.4;
}
h1 {
    font-size: 1.75rem;
}
h2 {
    font-size: 1.5rem;
}
h3 {
    font-size: 1.25rem;
}
h4,
h5,
h6 {
    font-size: 1rem;
}

p {
    line-height: 1.4;
    margin: 10px 0;
}

a,
a:link
a:visited,
a:focus {
    text-decoration: none;
    color: #000;
}
a:hover,
a:active {
    text-decoration: none;
    color: #000;
}

.clearfix:before,
.clearfix:after {
    content: " ";
    display: table;
    font-size: .1rem;
    line-height: .1;
}
.clearfix:after {
    clear: both;
}
/*
.clearfix {
    *zoom: 1;
}
*/

/*
Visually hidden:
http://icomoon.io/#docs
http://h5bp.com/
<a href="#">
    <span class="icon-home"></span>
    <span class="visuallyhidden">Home</span>
</a>
*/
.visuallyHidden {
    border: 0;
    clip: rect(0 0 0 0);
    height: 1px;
    margin: -1px;
    overflow: hidden;
    padding: 0;
    position: absolute;
    width: 1px;
}

