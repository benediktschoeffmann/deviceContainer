/* navigation top*/

.navigationTop {
    width: 100%;
    border-bottom: 1px solid #d5d5d5;
}
.navigationTop ul,
.navigationTop li {
    list-style-type: none;
    padding: 0;
    margin: 0;
}
.navigationTop ul {
    width: 100%;
    display: table;
    border-collapse: collapse;
    table-layout: fixed;
}
.navigationTop li {
    text-align: center;
    text-transform: uppercase;
    display: table-cell;
    border-left: 1px solid #d5d5d5;
}
.navigationTop li:first-child {
    border-left: none;
}
.navigationTop li a {
    font-size: .8125rem;
    font-weight: 700;
    text-decoration: none;
    display: block;
    padding: 8px 0;
    color: #000;
}
.navigationTop li a:hover {
    text-decoration: none;
    color: #000;
}

/* navigation main */

.navigationMainActive::-webkit-scrollbar {
    display: none;
}

.navigationMain {
    width: 0;
    /*height: 100%;*/
    /*height: auto;*/
    padding-top: 62px; /* die Hoehe von .header */
    padding-bottom: 22px;
    background: #3b3b3b;
    /*position: fixed;*/
    position: absolute;
    top: 0;
    left: 0;
    z-index: 10;
    overflow-x: hidden;
    overflow-y: auto;
    -webkit-transition: width 0.2s ease;
    transition: width 0.2s ease;
}

/* ----------------------------- */

.navigationMainItem {
    font-size: 1.25rem;
    text-decoration: none;
    display: block;
    padding: 4px 5%;
    border-bottom: 1px solid #616161;
    color: #c7c7c7;
}
.navigationMainItem:last-child {
    border-bottom: none;
}
.navigationMainItem,
.navigationMainItem:link,
.navigationMainItem:visited,
.navigationMainItem:focus {
    text-decoration: none;
    color: #c7c7c7;
}

/* ----------------------------- */

.navigationMainActive .navigationMain {
    width: 100%;
}
.navigationMainActive .contentContainer {
    width: 0;
    height: 0;
    overflow: hidden;
}

/* ----------------------------- */

.navigationMainActive .headerNavButton svg {
    display: block;
}
.navigationMainActive .headerNavButton svg:first-child {
    display: none;
}

/* ----------------------------- */

.navigationMainItem.latestCover {
    text-align: left;
}
.navigationMainItem.latestCover span,
.navigationMainItem.latestCover img {
    display: block;
    width: 100%;
}
.navigationMainItem.latestCover span {
    /*font-weight: 700;*/
    margin: 0 0 6px;
}



/* navigation bottom */
.navigationBottom {
    position: fixed;
    bottom: 0;
    left: 0;

    width: 100%;
    height: 60px;

    background-color: #fff;

    z-index: 2;

    -webkit-box-shadow: 0 0 7px #ccc;
    -moz-box-shadow: 0 0 7px #ccc;
    box-shadow: 0 0 7px #ccc;
}

.navigationBottom ul,
.navigationBottom li {
    list-style: none;
}
.navigationBottom ul {
    display: table;
    table-layout: fixed;
    border-collapse: collapse;
    width: 100%;
}
.navigationBottom li {
    display: table-cell;

    padding: 5px 0;
    border-left: 2px solid #ededed;

    text-align: center;
}
.navigationBottom li:first-child {
    border-left: none;
}

.navigationBottom li a {
    display: block;
    color: #aaa;
}
.navigationBottom li a:hover {
    color: #3478ff;
}

@media all and (min-width: 768px) {
    .navigationBottom {
        width: 70%;
        margin-left: 30%;
    }
}


/* header */

.wrapper > .header {
    display: block;
    width: 100%;
    height: 52px;
    position: fixed;
    z-index: 20;
    background-color: #d0113a;
    background-image: -webkit-linear-gradient(top, #e73239, #d0113a);
    background-image: linear-gradient(to bottom, #e73239, #d0113a);
}

/* ----------------------------- */

.headerNavButton,
