    /*padding-bottom: 62.58%;*/
}

.tabbedBox.madonna .contentBox.console .story .imageContainer img {
    height: auto;
}

.contentBox.console .storyText {
    width: 100%;
    padding: 2%;
    position: absolute;
    left: 0;
    bottom: 0;
    /*background-color: #000;*/
    /*background-color: rgba(0,0,0,.3);*/
    background: transparent linear-gradient(rgba(0,0,0,0) 0%, rgba(0,0,0,.8) 100%);
    overflow: hidden;
}
.contentBox.console .storyText.noShadow {
    background: transparent;
}
.contentBox.console .storyPreTitle,
.contentBox.console .storyTitle,
.contentBox.console .storyLeadText {
    color: #fff;
    display: block;
}
.contentBox.console .storyPreTitle {
    /*font-size: 1rem;*/
    font-size: 0.7rem;
    line-height: 1.4;
