/* articleSlideshow*/
.articleSlideshow .storyImage {
    margin-bottom: 10px;
}

/* VOTING */
.articleSlideshow #votingFrame {
    width: 85%;

    margin-bottom: 10px;
    margin-left: 15%;
}



/* articleVideo */
.articleBox.articleVideo .storyTextTop {
    margin: 6px 0;
}
.articleBox.articleVideo .storyLeadText {
    padding: 10px 0;
}


.storySocialMedia {
    opacity: 0;
    -webkit-transition: opacity 2s ease-in;
       -moz-transition: opacity 2s ease-in;
        -ms-transition: opacity 2s ease-in;
         -o-transition: opacity 2s ease-in;
            transition: opacity 2s ease-in;
}

.storySocialMedia.loaded {
    opacity: 1;
}

.storySocialMedia .mail-button {
    margin-right: 5px;
    display: block;
    background-color: #bababa;
}

.storySocialMedia .mail-button:hover {
    -webkit-transform: translateY(-4px);
    -ms-transform: translateY(-4px);
        transform: translateY(-4px);
    outline-offset: -1px;
    color: inherit;
}

.storySocialMedia .at-share-btn {
    position: relative;
    float: left;
    display: block !important;
    width: 15vw !important;
    height: 15vw !important;
    border-radius: 3px !important;
}

.storySocialMedia .at-share-btn svg {
    width: 100% !important;
    height: 100% !important;

    position: absolute;
    left: 50%;
    top: 50%;
    -webkit-transform: translate(-50%, -50%);
        -ms-transform: translate(-50%, -50%);
            transform: translate(-50%, -50%);
}

.storySocialMedia .mail-button.at-share-btn svg {
    width: 80% !important;
    height: 80% !important;
    padding: 20% 0 0 20%;

    left: 0;
    top: 0;
    -webkit-transform: translate(0);
        -ms-transform: translate(0);
            transform: translate(0);

    fill: #fff;
}


.bnApp .oe24AddThis {
    display: none;
}

.articleBox .spunQRelatedArticles .elementWrapper .link {
    color: #000;
}


.spunQRelatedArticles {
    width: 100%;
}


.spunQRelatedArticles .spunQRelatedArticlesHeadline {
    font-family: "Open Sans Condensed",sans-serif;
    font-size: 1.375rem;
    font-weight: 700;
    letter-spacing: -1pt;
    line-height: 1;
    text-transform: uppercase;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    padding: 3px 5px;
    margin: 0;
    color: #fff;
    background-color: #d0113a;
}


.spunQRelatedArticles .elements,
.spunQRelatedArticles .elementWrapper {
    list-style-type: none;
    padding: 0;
    margin: 0;
}


.spunQRelatedArticles .elementWrapper {
    padding: 2px 0 4px;
    /*margin: 0 0 5px 0;*/
    border-bottom: 1px solid #dedede;
}


.spunQRelatedArticles .elementWrapper,
.spunQRelatedArticles .elementWrapper .link {
    display: block;
}
.spunQRelatedArticles .elementWrapper .link,
.spunQRelatedArticles .elementWrapper .link:hover {
    font-size: .875rem;
}


.spunQRelatedArticles .elementWrapper .imageWrapper {
    float: left;
}
.spunQRelatedArticles .elementWrapper .imageWrapper {
    width: 33%;
    padding-top: 4px;
}
.spunQRelatedArticles .elementWrapper .textWrapper {
    width: 65%;
    margin-left: 35%;
}


.spunQRelatedArticles .elementWrapper .imageWrapper img {
    width: 100%;
    display: block;
}


.spunQRelatedArticles .elementWrapper .textWrapper .preTitle,
.spunQRelatedArticles .elementWrapper .textWrapper .title,
.spunQRelatedArticles .elementWrapper .textWrapper .leadText {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    display: block;
}
.spunQRelatedArticles .elementWrapper .textWrapper .preTitle {
    font-family: "Open Sans Condensed",sans-serif;
    font-size: .8125rem;
    text-transform: uppercase;
    font-weight: 700;
}
.spunQRelatedArticles .elementWrapper .textWrapper .title {
    font-family: "Open Sans Condensed",sans-serif;
    font-size: 1rem;
    font-weight: 700;
    line-height: 1.2;
}
