    max-height: 100%;

    position: absolute;
    right: 0;
    top: 50%;
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    transform: translateY(-50%);
}
.oe24tvTeaser.collapsed .teaserIconCross {
    display: none;
}
.oe24tvTeaser.collapsed .teaserIconPlus {
    display: block;
}



