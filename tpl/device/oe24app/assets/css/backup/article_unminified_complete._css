    float: left;
}
.oe24tvTeaser .teaserFigure {
    float: right;
}
.oe24tvTeaser .teaserFigure img {
    width: 100%;
}
.oe24tvTeaser .teaserFigure video {
    width: 100%;
}

.oe24tvTeaser .teaserPreTitle,
.oe24tvTeaser .teaserTitle {
    font-family: "oe24",sans-serif;
    line-height: 1.2;
    text-transform: uppercase;
    display: inline-block;
}
.oe24tvTeaser .teaserPreTitle {
    font-size: 14px;
    padding: 3px 5px 1px;
    margin: 0 0 10px;
    color: #d0113a;
    background-color: #fff;
}
.oe24tvTeaser .teaserTitle {
    font-size: 14px;
    font-weight: 700;
    padding: 0;
    margin: 0;
    color: #fff;
}


.oe24tvTeaser .teaserIcon {

    /*width: 8vw;*/
    /*height: 8vw;*/
    width: 10vw;
    height: 10vw;

    background-color: #d0113a;
    position: absolute;
    right: 0;
    /*top: -8vw;*/
    top: -10vw;

    /* border */
    right: -1px;
    border: 1px solid #ef3e64;
    border-bottom: none;
    /* border end */
}


.oe24tvTeaser .teaserIcon div {
    line-height: .5;
    display: none;
    width: 50%;
    height: 50%;
    position: absolute;
    left: 50%;
    top: 50%;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}


.oe24tvTeaser .teaserIcon svg {
    width: 100%;
    height: 100%;
    stroke: #fff;
}
.oe24tvTeaser .teaserIcon .teaserIconCross {
    display: block;
}
.oe24tvTeaser .teaserIcon .teaserIconPlus {
    display: none;
}



