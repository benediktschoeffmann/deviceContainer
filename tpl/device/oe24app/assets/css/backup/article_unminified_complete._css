/* bundestagswahl */

.smartphone .bundestagswahl {
    padding: 0;
    position: relative;
}

.smartphone .bundestagswahlNavButton {
    display: block;
    width: 44px;
    height: 44px;
    padding: 0;
    margin: 0;
    position: absolute;
    left: 224px;
    top: 0;
    z-index: 2;
    color: #fff;
}

.smartphone .bundestagswahlNavButton svg {
    width: 12px;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    fill: #fff;
    stroke: #fff;
}

.smartphone .bundestagswahlNav {
    width: 100%;
    background-color: #d0113a;
    position: absolute;
    left: 0;
    top: 45px;

    display: none;
    z-index: 0;

    opacity: 0;
    height: 0;

    transition: opacity .2s, height .2s;
}

.smartphone .bundestagswahl.bundestagswahlNavActive .bundestagswahlNav {
    display: block;
    z-index: 100;

    opacity: 1;
    height: auto;
}

.smartphone .bundestagswahlNavItem {
    display: block;
    font-size: 1rem;
    text-align: left;
    width: 100%;
    padding: 4px 10px;
    border-bottom: 1px solid #fff;
}

.smartphone .bundestagswahlNavItem,
.smartphone .bundestagswahlNavItem:visited,
.smartphone .bundestagswahlNavItem:active {
    text-decoration: none;
    color: #fff;
    background-color: #3b3b3b;
}

.smartphone .bundestagswahlNavItem:hover,
.smartphone .bundestagswahlNavItem.bundestagswahlNavCurrent {
    text-decoration: none;
    color: #fff;
    background-color: #3b3b3b;
}



